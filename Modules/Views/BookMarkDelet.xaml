<UserControl 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/" 
             xmlns:av="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="av" x:Class="MobilemedCrud.Registry.Views.BookMarkDelet"
             prism:ViewModelLocator.AutoWireViewModel="True" Height="400" Width="718">

    <Grid Background="Chartreuse">
        <Grid>
            <DataGrid ItemsSource="{Binding BookMarkModels}" SelectionChanged="DataGrid_SelectionChanged" Margin="0,0,-3,0"/>
            <TextBox HorizontalAlignment="Center" TextWrapping="Wrap" Text="Id da Consulta:" VerticalAlignment="Center" Width="120"/>
            <Button Content="Cancelar Consulta" Margin="498,0,129,0" VerticalAlignment="Center"/>
        </Grid>
    </Grid>
</UserControl>
